{
  "name": "n8n-nodes-hotjar",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Hotjar providing 10 resources and 60+ operations for surveys, survey responses, feedback, heatmaps, recordings, funnels, events, sites, users (GDPR), and organization management.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-hotjar.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-hotjar",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "hotjar",
    "analytics",
    "heatmaps",
    "surveys",
    "recordings",
    "user-feedback",
    "gdpr",
    "behavior-analytics",
    "funnels",
    "feedback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/HotjarApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Hotjar/Hotjar.node.js",
      "dist/nodes/Hotjar/HotjarTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.24.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
